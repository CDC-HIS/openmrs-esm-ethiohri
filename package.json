    "@ohri/openmrs-ohri-form-engine-lib": "^0.1",
    "@openmrs/esm-framework": "*",
    "carbon-components": "10.x",
    "carbon-icons": "7.x",
    "dayjs": "1.x",
    "react": "18.x",
    "react-i18next": "11.x",
    "react-router-dom": "6.x",
    "rxjs": "6.x"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.14.4",
