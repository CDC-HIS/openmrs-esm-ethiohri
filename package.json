    "@types/carbon__icons-react": "^10.31.0",
    "@types/jest": "^28.0.0",
    "@types/lodash": "^4.14.182",
    "@types/react": "^18.0.21",
    "@types/react-dom": "17.0.2",
    "@types/react-router": "^5.1.19",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/parser": "^5.14.0",
    "babel-eslint": "^10.1.0",
